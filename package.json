    "@sveltejs/vite-plugin-svelte": "^2.3.0",
    "@tanstack/svelte-query": "^4.29.7",
    "@tsconfig/svelte": "^4.0.1",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.14",
    "autosize": "^6.0.1",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "genversion": "^3.1.1",
    "highlight-words": "^1.2.2",
    "kaios-lib": "^1.6.0",
    "onyx-keys": "^1.2.0",
    "onyx-navigation": "^1.7.0",
