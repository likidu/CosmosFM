    "svelte-preprocess": "^5.0.3",
    "svelte-spa-router": "^3.3.0",
    "svelte-time": "^0.7.1",
    "tailwindcss": "^3.3.2",
    "tailwindcss-gradients": "^3.0.0",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
